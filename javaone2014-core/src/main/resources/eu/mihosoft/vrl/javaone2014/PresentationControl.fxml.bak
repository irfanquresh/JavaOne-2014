    <HBox prefHeight="-1.0" prefWidth="600.0" style="" styleClass="control-background" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <StackPane prefHeight="150.0" prefWidth="-1.0" HBox.hgrow="ALWAYS">
