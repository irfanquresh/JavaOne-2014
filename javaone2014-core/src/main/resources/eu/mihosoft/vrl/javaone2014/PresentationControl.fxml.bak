<AnchorPane id="AnchorPane" fx:id="root" maxHeight="-1.0" prefHeight="150.0" prefWidth="782.0" style="&#10;" xmlns:fx="http://javafx.com/fxml" fx:controller="eu.mihosoft.vrl.javaone2013.PresentationControlController">
